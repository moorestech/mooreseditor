{
  "$id": "inventoryConnectors",
  "$schema": "http://json-schema.org/draft-07/schema#",


  "type": "object",
  "required": ["offset", "directions"],
  "properties": {
    "offset": {
      "type": "array",
      "pattern": "@vector3Int",
      "items": {
        "type": "integer"
      }
    },
    "directions": {
      "type": "array",
      "items": {
        "type": "array",
        "pattern": "@vector3Int",
        "items": {
          "type": "integer"
        }
      }
    }
  }
}